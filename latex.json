            "  \\directlua{",
            "    function createDirectoryIfNotExists(directory)",
            "      local lfs = require('lfs')",
            "      local path = lfs.currentdir() .. '/' .. directory",
            "      local attributes = lfs.attributes(path)",
            "      if attributes == nil then",
            "        lfs.mkdir(path)",
            "        texio.write_nl('Created directory: ' .. directory)",
            "      else",
            "        texio.write_nl('Directory already exists: ' .. directory)",
            "      end",
            "    end",
            "    createDirectoryIfNotExists('#1')",
            "  }",
            "}",
